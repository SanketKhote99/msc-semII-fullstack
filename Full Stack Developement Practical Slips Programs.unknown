object on console.</p><p><br></p><p><br></p><p>// Import the mysql module</p><p>const mysql = require('mysql');</p><p><br></p><p>// Create a connection to the MySQL server</p><p>const connection = mysql.createConnection({</p><p>&nbsp; &nbsp; host: 'localhost', // Change this to your MySQL server host</p><p>&nbsp; &nbsp; user: 'root',&nbsp; &nbsp; &nbsp; // Change this to your MySQL username</p><p>&nbsp; &nbsp; password: 'password',&nbsp; // Change this to your MySQL password</p><p>&nbsp; &nbsp; database: 'your_database_name' // Change this to your MySQL database name</p><p>});</p><p><br></p><p>// Connect to the MySQL server</p><p>connection.connect((err) =&gt; {</p><p>&nbsp; &nbsp; if (err) {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; console.error('Error connecting to MySQL server:', err);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; return;</p><p>&nbsp; &nbsp; }</p><p><br></p><p>&nbsp; &nbsp; console.log('Connected to MySQL server');</p><p><br></p><p>&nbsp; &nbsp; // Select all records from the "students" table</p><p>&nbsp; &nbsp; connection.query('SELECT * FROM students', (err, rows) =&gt; {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; if (err) {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; console.error('Error executing query:', err);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; } else {</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; // Display the result object on the console</p><p>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; console.log('Result:', rows);</p><p>&nbsp; &nbsp; &nbsp; &nbsp; }</p><p><br></p><p>&nbsp; &nbsp; &nbsp; &nbsp; // Close the connection</p><p>&nbsp; &nbsp; &nbsp; &nbsp; connection.end();</p><p>&nbsp; &nbsp; });</p><p>});</p><div><br></div><p><b><span style="font-size: x-large;"><br></span></b></p><p><b><span style="font-size: x-large;"><br></span></b></p><p style="--tw-border-spacing-x: 0; --tw-border-spacing-y: 0; --tw-ring-color: rgba(69,89,164,.5); --tw-ring-offset-color: #fff; --tw-ring-offset-shadow: 0 0 transparent; --tw-ring-offset-width: 0px; --tw-ring-shadow: 0 0 transpa