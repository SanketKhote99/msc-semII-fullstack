.rc-anchor-light { background: rgb(249, 249, 249); color: rgb(0, 0, 0); }

.rc-anchor-light.rc-anchor-normal, .rc-anchor-light.rc-anchor-compact { border: 1px solid rgb(211, 211, 211); }

